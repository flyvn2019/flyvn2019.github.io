        script(type='text/javascript' src="js/flatpickr.js")

        script.
            $('.form-group_depart input').flatpickr({
                altInput: false,
                disableMobile: true,
                dateFormat: 'D, d-m-Y',
                minDate: 'today'
            });

            $('.form-group_return input').flatpickr({
                altInput: false,
                disableMobile: true,
                dateFormat: 'D, d-m-Y',
                minDate: 'today'
            });
        script.
            $('.nav-link').click(function () {

                $(this).next('.nav-sub').slideToggle(400);
                $('.nav-item.active .nav-sub').slideUp(400);

                $(this).parent('.nav-item').toggleClass('active');
                $(this).parent('.nav-item').siblings().removeClass('active');


            });
        script.
            $('.home-search__tab li button').click(function () {
                $(this).parent('li').addClass('active');
                $(this).parent('li').siblings().removeClass('active');
            });
            $('#btn1').click(function () {
                $('.form-group_return').removeClass('d-hidden');
                $('.multi').addClass('d-none');
                $('.no-multi').removeClass('d-none');
            });
            $('#btn2').click(function () {
                $('.form-group_return').addClass('d-hidden');
                $('.multi').addClass('d-none');
                $('.no-multi').removeClass('d-none');
            });
            $('#btn3').click(function () {
                $('.multi').removeClass('d-none');
                $('.no-multi').addClass('d-none');
            });
        script.
            $('.form-group_from input').click(function () {
                $(this).parents('.form-group_from').children('.search-default').toggleClass('d-none');
                $('.form-group_to .search-default').addClass('d-none');
            });
            $('.form-group_to input').click(function () {
                $(this).parents('.form-group_to').children('.search-default').toggleClass('d-none');
                $('.form-group_from .search-default').addClass('d-none');
            });
        script.
            $('.place-slide').slick({
                slidesToShow: 4,
                slidesToScroll: 1,
                //autoplay: true,
                speed: 500,
                arrows: true,
                dots: false
            });









