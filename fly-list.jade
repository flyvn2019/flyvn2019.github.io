        script.
            $('.form-group_depart input').flatpickr({
                altInput: false,
                disableMobile: true,
                dateFormat: 'D, d-m-Y',
                minDate: 'today'
            });

            $('.form-group_return input').flatpickr({
                altInput: false,
                disableMobile: true,
                dateFormat: 'D, d-m-Y',
                minDate: 'today'
            });
        script.
            $('.home-search__tab li button').click(function () {
                $(this).parent('li').addClass('active');
                $(this).parent('li').siblings().removeClass('active');
            });
            $('#btn1').click(function () {
                $('.form-group_return').removeClass('d-hidden');
                $('.multi').addClass('d-none');
                $('.no-multi').removeClass('d-none');
            });
            $('#btn2').click(function () {
                $('.form-group_return').addClass('d-hidden');
                $('.multi').addClass('d-none');
                $('.no-multi').removeClass('d-none');
            });
            $('#btn3').click(function () {
                $('.multi').removeClass('d-none');
                $('.no-multi').addClass('d-none');
            });
        script.
            $('.form-group_from input').click(function () {
                $(this).parents('.form-group_from').children('.search-default').toggleClass('d-none');
                $('.form-group_to .search-default').addClass('d-none');
            });
            $('.form-group_to input').click(function () {
                $(this).parents('.form-group_to').children('.search-default').toggleClass('d-none');
                $('.form-group_from .search-default').addClass('d-none');
            });
        script.
            $('.nav-link').click(function () {

                $(this).next('.nav-sub').slideToggle(400);
                $('.nav-item.active .nav-sub').slideUp(400);

                $(this).parent('.nav-item').toggleClass('active');
                $(this).parent('.nav-item').siblings().removeClass('active');


            });

        script.
            $('.date-slide').slick({
                slidesToShow: 7,
                slidesToScroll: 1,
                //autoplay: true,
                speed: 500,
                infinite: false,
                responsive: [
                    {
                        breakpoint: 1024,
                        settings: {
                            slidesToShow: 5,
                            slidesToScroll: 1
                        }
                    },
                    {
                        breakpoint: 767,
                        settings: {
                            slidesToShow: 3,
                            slidesToScroll: 1
                        }
                    }
                ]
            });
        script.
            $('.ticket-table__detail').slideUp();
            $('.ticket-table__cell').click(function () {
                $('.ticket-table__cell').removeClass('active');
                $(this).addClass('active');

                $(this).parents('.ticket-table__item').find('.ticket-table__detail').slideDown();
                $(this).parents('.ticket-table__item').siblings().find('.ticket-table__detail').slideUp();
            });
        script.
            if (screen.width >= 768) {
                var outboundBox = $('.outbound-box').offset().top;
                $(window).scroll(function () {
                    if ($(window).scrollTop() >= outboundBox) {
                        $('.outbound-box').addClass('fixed-top');
                    } else {
                        $('.outbound-box').removeClass('fixed-top');
                    }
                });
            };

        script.
            $('.ticket-box__show .p-show-detail').click(function () {
                $(this).parents('.ticket-box').find('.ticket-box__detail').slideToggle(400);
                $(this).parents('.ticket-box').toggleClass('open-detail');
            });
            $('.ticket-box__check').click(function () {
                $('.ticket-box__item').removeClass('active');
                $(this).parents('.ticket-box__item').addClass('active');
            });
