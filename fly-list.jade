        script.
            $('.nav-link').click(function () {

                $(this).next('.nav-sub').slideToggle(400);
                $('.nav-item.active .nav-sub').slideUp(400);

                $(this).parent('.nav-item').toggleClass('active');
                $(this).parent('.nav-item').siblings().removeClass('active');


            });

        script.
            $('.date-slide').slick({
                slidesToShow: 7,
                slidesToScroll: 1,
                //autoplay: true,
                speed: 500,
                infinite: false,
                responsive: [
                    {
                        breakpoint: 1024,
                        settings: {
                            slidesToShow: 5,
                            slidesToScroll: 1
                        }
                    },
                    {
                        breakpoint: 767,
                        settings: {
                            slidesToShow: 3,
                            slidesToScroll: 1
                        }
                    }
                ]
            });
        script.
            $('.ticket-table__cell').click(function () {
                $('.ticket-table__cell').removeClass('active');
                $(this).addClass('active');
            });

            $('.ticket-table__type').click(function () {
                $(this).parents('.ticket-table').find('.ticket-table__collapse').slideToggle(400);
            });


